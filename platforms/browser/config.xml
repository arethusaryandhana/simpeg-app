<?xml version='1.0' encoding='utf-8'?>
<widget id="pttati.simpeg.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>SIM-AKU</name>
    <icon density="mdpi" height="57" platform="android" src="www/img/icon_kutim.png" width="57" />
    <description>
        Made By Arethusa Aryandhana
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        Arethusa Aryandhana
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent action="http://*/*" />
    <allow-intent action="https://*/*" />
    <allow-intent action="tel:*" />
    <allow-intent action="sms:*" />
    <allow-intent action="mailto:*" />
    <allow-intent action="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
